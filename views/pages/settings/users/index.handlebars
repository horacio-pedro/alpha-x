<!-- Loading starts -->
{{>_loader}}
<!-- Loading ends -->

<div class="container">
    
    <!-- *************
************ Header section start *************
************* -->
    <!-- Header start -->
    {{>_header}}
    <!-- Header end -->
    <!-- Navigation start -->
    {{>navigation}}
    <!-- Navigation end -->

    <!-- Search bar start -->
    {{>_searchBar}}
    <!-- Search bar end -->

    <!-- *************
************ Header section end *************
************* -->

    <!-- *************
************ Main container start *************
************* -->
    <div class="main-container">


        <!-- Page header start -->
        <div class="page-title">
            <div class="row gutters">
                <div class="col-xl-6 col-lg-6 col-md-6 col-sm-12 col-12">
                    <h5 class="title">Utilizadores</h5>
                </div>
                <div class="col-xl-6 col-lg-6 col-md-6 col-sm-12 col-12">
                    <div class="daterange-container">
                        <div class="date-range">
                            <div>
                                <i class="icon-calendar" 
                                style="font-size: 1.2rem;color:#00b894"></i> 
                                <script>writedate()</script>
                                <i class="icon-clock" 
                                style="font-size: 1.2rem;color:#00b894"></i>
                                <script>writeclock()</script>
                                <span class="range-text"></span>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- Page header end -->


        <!-- Content wrapper start -->
        <div class="content-wrapper">


            <!-- ************************** Visitors and Revenue ************************** -->
            <!-- Row start -->
            <div class="row gutters justify-content-center">
                {{>_msg}}
                <h4>Tabela de utilizadores</h4>
            </div>
            <!-- Row end -->

        </div>
        <!-- Content wrapper end -->


    </div>
    <!-- *************
************ Main container end *************
************* -->


</div>